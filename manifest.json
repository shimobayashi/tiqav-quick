{
  "name": "tiqav-quick",
  "version": "0.2",
  "manifest_version": 3,
  "description": "You can pick up awesome image as Markdown from tiqav so quickly!",
  "icons": {
      "48": "icon48.png",
      "128": "icon128.png"
  },
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "background",
    "clipboardWrite",
    "http://*.tiqav.com/"
  ]
}
